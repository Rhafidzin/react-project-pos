{ 
   "compilerOptions":{ 
      "allowJs": true,
      "noEmit": true,
      "target":"es6",
      "moduleResolution":"node",
      "module":"commonjs",
      "declaration":false,
      "noLib":false,
      "emitDecoratorMetadata":true,
      "experimentalDecorators":true,
      "sourceMap":true,
      "pretty":true,
      "allowUnreachableCode":true,
      "allowUnusedLabels":true,
      "noImplicitAny":true,
      "noImplicitReturns":false,
      "noImplicitUseStrict":false,
      "outDir":"dist/",
      "baseUrl": ".",
      "paths": {
        "@/*": [
          "./src/*"
        ]
      },
      "listFiles":false,
      "noEmitHelpers":true,
      
   },
   "include":[ 
      "**/*"
   ],
   "exclude":[ 
      "node_modules"
   ],
   "compileOnSave":false
}